{
    "name": "smartkitchen",
    "version": "1.0.0",
    "private": true,
    "description": "Smart Kitchen - Full Stack Project",
    "scripts": {
        "install-all": "npm install --prefix backend && npm install --prefix frontend",
        "start": "concurrently \"npm start --prefix backend\" \"npm start --prefix frontend\"",
